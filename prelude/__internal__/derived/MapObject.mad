import L from './List'
import O from './Object'
import P from './Map'

export toObject = L.reduce(
  (agg, kv) => (
    where (kv)
      is <k, v>: O.assign(agg, objOf(k, v)) 
  ),
  {}
)

fromObject :: x -> Map k v
export fromObject = (o) => pipe(
  O.entries,
  L.map((px) => < (#- px[0] -#), (#- px[1] -#) >),
  P.fromList
)(o)

